n=int(input())
a=[]
sum=0
for i in range(0,n):
    l=[]
    for j in range(0,n):
        x=int(input())
        l.append(x)
        a.append(l)
    for i in range(n):
        for j in range(n):
            if i+j==n-1:
                sum1=sum1+a[i][j]
            if i==j:
                sum2=sum2+a[i][j]
    print('sum')            
